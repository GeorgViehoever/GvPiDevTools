#!/bin/bash -e
#
# Sync and merge to master from upstream in current branch, and push to clones

doit () {
     currentBranch=$(git symbolic-ref --short HEAD)
     git pull --rebase upstream master
     git push origin $currentBranch
}

cd $PCLDIR
doit

cd $PJSRDIR
doit

cd $XISFDIR
doit

cd $PIDEVTOOLSDIR
doit

